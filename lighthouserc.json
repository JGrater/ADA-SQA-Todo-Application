{
    "ci": {
        "collect": {
            "numberOfRuns": 5,
            "startServerCommand": "npm start",
            "url": ["http://localhost:3000/"],
            "settings": {
                "onlyCategories": [
                    "performance",
                    "accessibility"
                ]
            }
        },
        "upload": {
            "target": "temporary-public-storage"
        }
    }
}
